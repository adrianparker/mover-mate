// This is your Prisma schema file,
// learn more about it in the docs: https://pris.ly/d/prisma-schema

generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "sqlite"
  url      = env("DATABASE_URL")
}

model Item {
  id      Int     @id @default(autoincrement())
  number  String?
  color   String?
  content String?
  room    String  @default("master")
  fragil  String?
  hand    String?
}
